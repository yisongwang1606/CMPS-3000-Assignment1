<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zb.mapper.ItemMapper">

    <resultMap id="itemResultMap" type="com.zb.entity.Item">
    </resultMap>

    <select id="getPriceList" resultMap="itemResultMap">
        select * from price_list
    </select>

    <select id="getSingleItem" resultMap="itemResultMap">
        select * from price_list where name = #{name}
    </select>



</mapper>